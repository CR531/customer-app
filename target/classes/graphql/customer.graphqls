type Customer {
	customer_id: String!,
	first_name: String,
	last_name: String,
	email_id: String,
	phone_number: Float,
	creation_date: String
}

type Query {
	customers(count: Int):[Customer]
	customerbyId(customer_id: String):Customer
}

type Mutation {
	createCustomer(customer_id: String!, first_name: String!, last_name: String!, email_id: String, phone_number: Float, creation_date: String):Customer
}